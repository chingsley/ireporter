{
  "name": "ireporter",
  "version": "1.0.0",
  "description": "a web app for tracking corruption and crisis",
  "main": "index.js",
  "scripts": {
    "dev-start": "babel-watch ./server/index.js",
    "start": "npm run build && node ./dist/index.js",
    "build": "babel ./server -d dist",
    "test": "export NODE_ENV=test && nyc --reporter=lcov mocha --recursive ./tests/* --require babel-register --exit",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "export NODE_ENV=test && nyc mocha --recursive ./tests/* --require babel-register --exit",
    "dev": "nodemon --exec babel-node ./server/index.js",
    "lint test": "eslint tests --fix",
    "lint-server": "eslint server",
    "lint-server--fix": "eslint server --fix"
  },
  "author": "Eneja Kingsley C",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1"
  }
}
